1. A) function num = getFacWhile(n)
num = 1;
while(n > 0)
   num = num * n;
   n = n - 1;
   end
 end
 B) function fac = getFacFor(n)
 fac = 1;
 i =1; 
 for(i:n)
 fac = fac * i
 end
 end
 C) function fac =getFacVec(n)
 vect = zeros(1,length(n));
 fac = prod(vect, i);
 end
 2. function time = timeFacFuncs
 disp(['average runtime for getFacWhile: ', num2str(timeit(getFacWhile))]);
 disp(['average runtime for getFacFor: ', num2str(timeit(getFacFor))]);
 disp(['average runtime for getFacVec: ', num2str(timeit(getFacVec))]);
